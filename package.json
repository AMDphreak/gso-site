{
  "name": "gso-site-monorepo",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter gso-site dev & pnpm --filter docs dev",
    "dev:site": "pnpm --filter gso-site dev",
    "dev:docs": "pnpm --filter docs dev",
    "build": "pnpm --filter gso-site build && pnpm --filter docs build",
    "build:site": "pnpm --filter gso-site build",
    "build:docs": "pnpm --filter docs build",
    "preview": "pnpm --filter gso-site preview",
    "preview:site": "pnpm --filter gso-site preview",
    "preview:docs": "pnpm --filter docs preview"
  },
  "devDependencies": {
    "pnpm": "^10.28.2"
  }
}
